'.text.srt':
  'Time':
    'prefix': 'time'
    'body': '${1:00}:${2:00}:${3:00},${4:000} --> ${5:00}:${6:00}:${7:00},${8:000}\n'
  'Bold':
    'prefix': 'b'
    'body': '<b>$1</b>'
  'Italic':
    'prefix': 'i'
    'body': '<i>$1</i>'
  'Underline':
    'prefix': 'u'
    'body': '<u>$1</u>'
  'Color':
    'prefix': 'f'
    'body': '<font color="$1">$2</font>'
